{
    "AWSEBDockerrunVersion": "1",
    "containerDefinitions": [
        {
            "environment": [
                {
                    "name": "NODE_ENV",
                    "value": "production"
                }
            ],
            "essential": true,
            "mountPoints": [
                {
                    "containerPath": "/usr/src/app",
                    "sourceVolume": "_"
                },
                {
                    "containerPath": "/usr/src/app/node_modules",
                    "sourceVolume": "UsrSrcAppNode_Modules"
                }
            ],
            "name": "web",
            "image": "151708502202.dkr.ecr.ap-southeast-1.amazonaws.com/coders-mojo-backend-staging-web:latest",
            "memory": 128,
            "portMappings": [
                {
                    "containerPort": 3000,
                    "hostPort": 3000
                }
            ]
        }
    ],
    "family": "",
    "volumes": [
        {
            "host": {
                "sourcePath": "."
            },
            "name": "_"
        },
        {
            "host": {
                "sourcePath": "/usr/src/app/node_modules"
            },
            "name": "UsrSrcAppNode_Modules"
        }
    ]
}